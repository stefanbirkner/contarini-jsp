<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd"
        version="2.1">

    <display-name>Contarini Tag Library</display-name>
    <tlib-version>0.1.0</tlib-version>
    <short-name>contarini</short-name>
    <uri>http://stefanbirkner.github.com/contarini/tags</uri>

    <tag>
        <name>webCrawlerInfo</name>
        <tag-class>com.github.stefanbirkner.contarini.jsp.WebCrawlerInfoTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>info</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <type>com.github.stefanbirkner.contarini.WebCrawlerInfo</type>
        </attribute>
    </tag>
</taglib>